//- mixins for the reusable html parts
mixin card()
  #{attributes.tag}(class="card" class="card--#{attributes.game}")
    a.card__link(href="#{attributes.link}")
      .card__wrapper
        if(attributes.logo)
          .card__logo
            img(src="img/logo-#{attributes.logo}.svg")
        div.card__info-wrapper
          h4.card__title
            div
              | !{attributes.titleUp}
            div
              | !{attributes.titleDown}
          p.card__description
            | !{attributes.description}
    img.card__image(
      src="img/card-#{attributes.game}.png",
      style="opacity:0;transition: all .3s ease",
      onload="this.style.opacity='1'"
    )

mixin game()
  li.game
    a(class='game__image-link', href='#{attributes.link}')
      img.game__image(src="img/oldgame-#{attributes.img}.png")
    h4.game__title
      | !{attributes.title}
    p.game__description
      | !{attributes.description}
    a(onmouseover="this.parentNode.classList.add('active')" onmouseout="this.parentNode.classList.remove('active')" class='game__link', class='#{attributes.color}', href='#{attributes.link}')
      svg(xmlns='http://www.w3.org/2000/svg', viewBox='0 0 192 512')
        path(fill='rgb(255,255,255)', d='M166.9 264.5l-117.8 116c-4.7 4.7-12.3 4.7-17 0l-7.1-7.1c-4.7-4.7-4.7-12.3 0-17L127.3 256 25.1 155.6c-4.7-4.7-4.7-12.3 0-17l7.1-7.1c4.7-4.7 12.3-4.7 17 0l117.8 116c4.6 4.7 4.6 12.3-.1 17z')

mixin logo-clients()
  .clients__image-wrapper
    img.clients__image(src="img/logo-clients-#{attributes.index}.svg")

mixin laptop(img_src)
  img(class="services__image",  class="services__image--laptop", class="services__image--laptop-#{img_src}", src="img/about-laptop-#{img_src}.svg")

